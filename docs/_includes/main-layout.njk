<!doctype html>
<html lang="en">
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>{{ title }}</title>
  <link rel="stylesheet" href="/assets/css/docs.css">
  <link rel="apple-touch-icon" sizes="57x57" href="/assets/images/apple-icon-57x57.png">
  <link rel="apple-touch-icon" sizes="60x60" href="/assets/images/apple-icon-60x60.png">
  <link rel="apple-touch-icon" sizes="72x72" href="/assets/images/apple-icon-72x72.png">
  <link rel="apple-touch-icon" sizes="76x76" href="/assets/images/apple-icon-76x76.png">
  <link rel="apple-touch-icon" sizes="114x114" href="/assets/images/apple-icon-114x114.png">
  <link rel="apple-touch-icon" sizes="120x120" href="/assets/images/apple-icon-120x120.png">
  <link rel="apple-touch-icon" sizes="144x144" href="/assets/images/apple-icon-144x144.png">
  <link rel="apple-touch-icon" sizes="152x152" href="/assets/images/apple-icon-152x152.png">
  <link rel="apple-touch-icon" sizes="180x180" href="/assets/images/apple-icon-180x180.png">
  <link rel="icon" type="image/png" sizes="192x192"  href="/assets/images/android-icon-192x192.png">
  <link rel="icon" type="image/png" sizes="32x32" href="/assets/images/favicon-32x32.png">
  <link rel="icon" type="image/png" sizes="96x96" href="/assets/images/favicon-96x96.png">
  <link rel="icon" type="image/png" sizes="16x16" href="/assets/images/favicon-16x16.png">
  <meta name="msapplication-TileColor" content="#ffffff">
  <meta name="msapplication-TileImage" content="/assets/images/ms-icon-144x144.png">
  <meta name="theme-color" content="#ffffff">
  <link href="https://fonts.googleapis.com/css2?family=Almarai:wght@300;700&display=swap" rel="stylesheet">
  <link href="https://fonts.googleapis.com/css2?family=Poppins:ital,wght@0,600;1,300&display=swap" rel="stylesheet">
</head>
<body class="has-navbar-fixed-left">
<nav class="navbar-fixed-left" role="navigation" aria-label="main navigation">
    {%- for entry in collections.all | eleventyNavigation %}
      <a class="{% if entry.url == page.url %}active{% endif %}" href="{{ entry.url | url }}">
        {% if entry.icon %}
          <i class="{{ entry.icon }}"></i>
        {% endif %}
        <span>{{ entry.title }}</span>
      </a>
      {% if entry.children.length %}
        <div class="navbar-subitem">
          {%- for entryChildren in entry.children %}
            <a class="{% if entryChildren.url == page.url %}active{% endif %}" href="{{ entryChildren.url | url }}">
              <span> --> {{ entryChildren.title }}</span>
            </a>
          {%- endfor %}
        </div>
      {% endif %}
    {%- endfor %}
    <div class="push-bottom">
      <a href="#">
        <i class="icon-github"></i> <span>Contribute</span>
      </a>
    </div>
  </nav>
  {{ content | safe }}
  <script>
    (function(){
      document.querySelectorAll(".navbar-subitem a.active").forEach( function(element) {
        element.closest('.navbar-subitem').classList.add('active');
      });
    })()
  </script>
</body>
</html>
